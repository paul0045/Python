# Creating a dictionary
my_dict = {
    'name': 'Alice',
    'age': 25,
    'city': 'New York'
}
print("Created dictionary:", my_dict)
# Accessing values
name = my_dict['name']
age = my_dict['age']
city = my_dict['city']
print(f"Name: {name}, Age: {age}, City: {city}")
# Deleting an item using del
del my_dict['city']
print("Dictionary after deleting 'city':", my_dict)

# Deleting an item using pop
age = my_dict.pop('age')
print(f"Popped age: {age}")
print("Dictionary after popping 'age':", my_dict)
# Updating an existing key
my_dict['name'] = 'Bob'
print("Dictionary after updating 'name':", my_dict)

