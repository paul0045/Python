# Creating Tuples
my_tuple = (1, 2, 3, 4, 5)
print("Created tuple:", my_tuple)

another_tuple = 6, 7, 8, 9, 10
print("Another tuple:", another_tuple)

single_element_tuple = (1,)
print("Single element tuple:", single_element_tuple)

# Accessing Elements in a Tuple
print("First element:", my_tuple[0])
print("Last element:", my_tuple[-1])
print("Elements from index 1 to 3:", my_tuple[1:4])

# Iterating Over a Tuple
for element in my_tuple:
    print("Element:", element)

# Tuple Unpacking
a, b, c, d, e = my_tuple
print("Unpacked values:", a, b, c, d, e)

tuple_example = (1, 2, 3)
x, y, z = tuple_example
print("Unpacked values from tuple_example:", x, y, z)

# Combining and Repeating Tuples
combined_tuple = my_tuple + another_tuple
print("Combined tuple:", combined_tuple)

repeated_tuple = my_tuple * 2
print("Repeated tuple:", repeated_tuple)

# Checking for Membership
print("Is 3 in my_tuple?", 3 in my_tuple)
print("Is 6 in my_tuple?", 6 in my_tuple)

# Built-in Tuple Functions
print("Length of my_tuple:", len(my_tuple))
print("Maximum value in my_tuple:", max(my_tuple))
print("Minimum value in my_tuple:", min(my_tuple))
print("Sum of elements in my_tuple:", sum(my_tuple))
